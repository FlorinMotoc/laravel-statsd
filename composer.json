{
    "name": "florinmotoc/laravel-statsd",
    "description": "Laravel statsd",
    "require": {
        "php": ">=8.1",
        "florinmotoc/php-statsd-client": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "FlorinMotoc\\LaravelStatsd\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FlorinMotoc\\LaravelStatsd\\LaravelStatsdServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
